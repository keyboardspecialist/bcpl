  \name "trumpet1"
  $trumpet;
  \transposition(bes)

    %  |        |        |        |        |        |        |       |
    4c1 ||
%    |  r2                                  r8       b\v80    cis     b
%    |  dis4              cis8     b~       b        a4               gis8~
%    |  gis      b4.                        r8       b4               cis8
